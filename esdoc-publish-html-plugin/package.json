{
  "name": "esdoc-latest-publish-html-plugin",
  "version": "1.1.4",
  "description": "A publish HTML plugin for ESDoc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/esdoc-latest/esdoc-plugins"
  },
  "dependencies": {
    "@babel/generator": "^7.14.8",
    "cheerio": "^1.0.0-rc.10",
    "escape-html": "1.0.3",
    "fs-extra": "^9.1.0",
    "ice-cap": "0.0.4",
    "marked": "^2.1.3",
    "taffydb": "2.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/register": "^7.14.5",
    "esdoc-accessor-plugin": "^1.0.0",
    "esdoc-latest": "^1.0.1",
    "esdoc-latest-accessor-plugin": "^1.0.0",
    "esdoc-latest-brand-plugin": "^1.0.0",
    "esdoc-latest-coverage-plugin": "^1.1.0",
    "esdoc-latest-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-latest-external-ecmascript-plugin": "^1.0.0",
    "esdoc-latest-integrate-manual-plugin": "^1.0.0",
    "esdoc-latest-integrate-test-plugin": "^1.0.0",
    "esdoc-latest-type-inference-plugin": "^1.0.1",
    "esdoc-latest-undocumented-identifier-plugin": "^1.0.0",
    "esdoc-latest-unexported-identifier-plugin": "^1.0.0",
    "mocha": "^9.0.3"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "build": "rm -rf ./out/src && babel --out-dir out/src --ignore 'Builder/template' src && cp -a src/Builder/template out/src/Builder/",
    "test": "rm -rf ./test/fixture/out && mocha -t 10000 --require ./node_modules/@babel/register --require ./test/src/init.js --recursive ./test/src -R spec"
  },
  "keywords": [
    "esdoc",
    "publish",
    "html",
    "plugin"
  ],
  "files": [
    "out/src",
    "README.md"
  ],
  "main": "out/src/Plugin.js"
}
